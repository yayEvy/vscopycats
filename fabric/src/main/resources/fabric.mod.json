{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "{$mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "mod.everest.evy.vscopycats.fabric.VSCopyCats"
      }
    ],
    "client": [
      {
        "value": "mod.everest.evy.vscopycats.fabric.Client"
      }
    ],
    "preLaunch": [
      {
        "value": "mod.everest.evy.vscopycats.fabric.ValkyrienPreLaunch"
      }
    ]
  },
  "mixins": [
    "vs_addon_template-common.mixins.json",
    "vs_addon_template.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "valkyrienskies": ">=${vs2_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "architectury": ">=${architectury_version}"
  }
}